[
  {
    "name": "core (tree-shaken useForm)",
    "path": "index.js",
    "import": "{ useForm }",
    "ignore": ["react", "react-dom", "zustand"],
    "limit": "6 kB",
    "brotli": true
  },
  {
    "name": "plugin (createBackendSync)",
    "path": "plugins/backend-sync/index.js",
    "import": "{ createBackendSync }",
    "ignore": ["react", "react-dom", "zustand"],
    "limit": "3 kB",
    "brotli": true
  },
  {
    "name": "hooks (tree-shaken useForm + useIndexSnapshot)",
    "path": "hooks/index.js",
    "import": "{ useForm, useIndexSnapshot }",
    "ignore": ["react", "react-dom", "zustand"],
    "limit": "4 kB",
    "brotli": true
  }
]
